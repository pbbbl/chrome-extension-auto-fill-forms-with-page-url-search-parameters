{
    "manifest_version": 2,
    "name": "Auto-Fill Forms with URL Query Parameters",
    "description": "Automatically fill forms with URL query parameters",
    "homepage_url": "https://extensions.pbbbl.com/extension/auto-fill-forms-with-page-url-search-parameters",
    "version": "1.0",
    "icons": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
    },
    "browser_action": {
        "default_icon": "icon.png",
        "default_title": "Click here to load form data!",
        "default_popup": "index.html"
    },
    "permissions": [
        "tabs",
        "http://*/*",
        "https://*/*"
    ]
}